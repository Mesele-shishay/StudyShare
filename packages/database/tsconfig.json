{
    "extends": "../typescript-config/base.json",
    "compilerOptions": {
        "outDir": "dist",
        "rootDir": "src",
        "module": "CommonJS",
        "target": "ES2020",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "generated"
    ]
}